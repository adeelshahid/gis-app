{
  "name": "gis-app",
  "version": "27.0.0",
  "description": "GIS Web App for DHIS 2",
  "main": "src/app.js",
  "scripts": {
    "validate": "npm ls --depth 0",
    "start": "./node_modules/.bin/d2-manifest --href='http://localhost:8080' --manifest.activities.dhis.auth='admin:district' package.json manifest.webapp && webpack-dev-server",
    "build": "rm -rf build && webpack --config webpack.config.prod.js && cp -r dhis2 build && cp -r extjs build && mkdir -p build/src/custom && cp -r src/custom build/src && cp index.html build && cp -r css build/css && cp src/app.js build/src/app.js && cp manifest.webapp build/manifest.webapp && cp -r i18n build && cp -r images build",
    "lint": "./node_modules/eslint/bin/eslint.js src",
    "test": "TZ=UTC jest",
    "test-watch": "npm run test -- --watch",
    "test-cov": "npm test -- --coverage",
    "posttest-cov": "open ./coverage/phantomjs/lcov-report/index.html",
    "components:start": "webpack-dev-server --config webpack.components.config.js",
    "components:test": "NODE_ENV=test nyc --require babel-register --require babel-polyfill mocha --reporter dot --recursive test/components && nyc report --reporter=lcov",
    "components:watch": "NODE_ENV=test nyc --require babel-register --require babel-polyfill mocha --watch --reporter dot --recursive test/components && nyc report --reporter=lcov",
    "postcomponents:test": "open coverage/lcov-report/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhis2/gis-app.git"
  },
  "keywords": [
    "DHIS2",
    "HISP",
    "GIS"
  ],
  "author": "Bj√∏rn Sandvik",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/dhis2/gis-app/issues"
  },
  "homepage": "https://github.com/dhis2/gis-app#readme",
  "dependencies": {
    "d2": "^28.2.0",
    "d2-ui": "^28.0.3",
    "d2-utilizr": "^0.2.15",
    "d3-color": "^1.0.3",
    "d3-time-format": "^2.0.5",
    "dhis2-gis-api": "^27.0.1",
    "file-saver": "^1.3.3",
    "i18next": "^10.0.7",
    "i18next-xhr-backend": "^1.5.0",
    "lodash": "^4.17.4",
    "loglevel": "^1.5.0",
    "material-ui": "^0.19.1",
    "promise-polyfill": "6.0.2",
    "prop-types": "^15.5.10",
    "react": "^16.1.1",
    "react-color": "^2.13.5",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-sortable-hoc": "^0.6.8",
    "react-virtualized": "^9.13.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-observable": "^0.16.0",
    "redux-thunk": "^2.2.0",
    "rxjs": "^5.4.3",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "css-loader": "^0.28.7",
    "d2-manifest": "^1.0.0",
    "enzyme": "^2.9.1",
    "eslint": "^4.6.1",
    "eslint-config-dhis2": "^2.1.0",
    "eslint-plugin-react": "^7.3.0",
    "exports-loader": "^0.6.4",
    "file-loader": "^0.11.2",
    "gifsicle": "^3.0.4",
    "html-webpack-plugin": "^2.30.1",
    "i18next-conv": "^5.0.0",
    "image-webpack-loader": "^3.4.2",
    "imports-loader": "^0.7.1",
    "isparta-loader": "^2.0.0",
    "jest": "^21.2.1",
    "node-sass": "^4.5.3",
    "nyc": "^11.2.1",
    "optipng-bin": "^4.0.0",
    "phantomjs": "^2.1.7",
    "phantomjs-polyfill": "^0.0.2",
    "pngquant-bin": "^3.1.1",
    "precommit-hook": "3.0.0",
    "react-addons-test-utils": "^15.6.0",
    "recursive-readdir": "^2.2.1",
    "redux-devtools": "^3.4.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "susy": "^3.0.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1",
    "webpack-uglify-js-plugin": "^1.1.9"
  },
  "pre-commit": [
    "test"
  ],
  "manifest.webapp": {
    "name": "DHIS 2 Maps App",
    "icons": {
      "48": "icon.png"
    },
    "activities": {
      "dhis": {
        "href": ".."
      }
    }
  }
}
