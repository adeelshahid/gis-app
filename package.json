{
  "name": "gis-app",
  "version": "27.0.0",
  "description": "GIS Web App for DHIS 2",
  "main": "src/app.js",
  "scripts": {
    "validate": "npm ls --depth 0",
    "start": "webpack-dev-server",
    "build": "rm -rf build && webpack --config webpack.config.prod.js && cp -r dhis2 build && cp -r extjs build && mkdir -p build/src/custom && cp -r src/custom build/src && cp index.html build && cp -r css build/css && cp src/app.js build/src/app.js && cp manifest.prod.webapp build/manifest.webapp && cp -r i18n build && cp -r images build",
    "lint": "./node_modules/eslint/bin/eslint.js src",
    "test": "./node_modules/karma-cli/bin/karma start test/karma.config.js --single-run",
    "test-watch": "./node_modules/karma-cli/bin/karma start test/karma.config.js",
    "test-cov": "npm test",
    "posttest-cov": "open ./coverage/phantomjs/lcov-report/index.html",
    "components:start": "webpack-dev-server --config webpack.components.config.js",
    "components:test": "NODE_ENV=test nyc --require babel-register --require babel-polyfill mocha --reporter dot --recursive test/components && nyc report --reporter=lcov",
    "components:watch": "NODE_ENV=test nyc --require babel-register --require babel-polyfill mocha --watch --reporter dot --recursive test/components && nyc report --reporter=lcov",
    "postcomponents:test": "open coverage/lcov-report/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhis2/gis-app.git"
  },
  "keywords": [
    "DHIS2",
    "HISP",
    "GIS"
  ],
  "author": "Bj√∏rn Sandvik",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/dhis2/gis-app/issues"
  },
  "homepage": "https://github.com/dhis2/gis-app#readme",
  "dependencies": {
    "d2-utilizr": "^0.2.7",
    "d3-time-format": "^2.0.1",
    "dhis2-gis-api": "^27.0.1",
    "fixed-data-table": "^0.6.3",
    "material-ui": "^0.16.7",
    "npm": "^4.1.2",
    "promise-polyfill": "6.0.2",
    "react-redux": "^5.0.2",
    "react-sortable-hoc": "0.4.12",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "whatwg-fetch": "^2.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.0",
    "babel-plugin-istanbul": "3.1.1",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "6.1.18",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "6.1.18",
    "chai": "3.4.1",
    "css-loader": "0.18.0",
    "enzyme": "^2.7.0",
    "eslint": "^3.10.2",
    "eslint-config-dhis2": "0.0.5",
    "eslint-plugin-react": "3.3.1",
    "exports-loader": "0.6.2",
    "file-loader": "^0.8.4",
    "image-webpack-loader": "^1.6.2",
    "imports-loader": "^0.6.5",
    "isparta-loader": "1.0.0",
    "karma": "^1.3.0",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "0.2.1",
    "karma-cli": "0.1.1",
    "karma-coverage": "0.5.3",
    "karma-mocha": "0.2.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sinon": "1.0.4",
    "karma-sinon-chai": "1.1.0",
    "karma-sourcemap-loader": "0.3.6",
    "karma-webpack": "^1.8.0",
    "mocha": "2.3.4",
    "node-sass": "3.4.2",
    "nyc": "^10.0.0",
    "phantomjs": "1.9.18",
    "phantomjs-polyfill": "0.0.1",
    "precommit-hook": "3.0.0",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "redux-devtools": "^3.3.2",
    "sass-loader": "2.0.1",
    "sinon": "1.17.2",
    "sinon-chai": "2.8.0",
    "style-loader": "0.12.3",
    "susy": "2.2.5",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.27",
    "webpack-dev-server": "^2.1.0-beta.12"
  },
  "pre-commit": [
    "test",
    "lint"
  ]
}
